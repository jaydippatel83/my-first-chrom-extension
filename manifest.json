{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0.0",
    "description": "A simple Chrome extension.",
    "permissions": [
        "storage",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js",
        "type":"module"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": { 
            "48": "images/icon32.png"
        }
    }, 
    "icons": { 
        "48": "images/icon32.png"
    }
}